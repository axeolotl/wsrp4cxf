<web-app>
    <display-name>WSRP4J Consumer Portlet</display-name>

    <!-- Resource Proxy -->
    <servlet>
        <servlet-name>WSRP4JResourceProxy</servlet-name>
        <servlet-class>
            org.apache.wsrp4j.consumer.proxyportlet.impl.ResourceProxy
        </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WSRP4JResourceProxy</servlet-name>
        <url-pattern>/WSRP4JResourceProxy/*</url-pattern>
    </servlet-mapping>
</web-app>
